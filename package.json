{
  "name": "build-week-scaffolding-node",
  "version": "1.0.0",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "up": "knex migrate:up",
    "down": "knex migrate:down",
    "downh": "heroku run knex migrate:down -a theonewhoknocks",
    "uph": "heroku run knex migrate:up -a theonewhoknocks",
    "migrateh": "heroku run knex migrate:latest -a theonewhoknocks",
    "rollbackh": "heroku run knex migrate:rollback -a theonewhoknocks",
    "databaseh": "heroku pg:psql -a theonewhoknocks",
    "seedh": "heroku run knex seed:run -a theonewhoknocks",
    "test": "cross-env NODE_ENV=testing jest --watchAll --verbose --runInBand",
    "heroku-postbuild": "cd client && npm i && npm run build",
    "deploy": "git push heroku main"
  },
  "engines": {
    "node": "14.16.0",
    "npm": "6.14.11"
  },
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dbconfig": "^1.1.7",
    "dotenv": "=8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.3",
    "knex-cleaner": "^1.3.1",
    "path": "^0.12.7",
    "pg": "=8.5.1",
    "sqlite3": "^5.0.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/jest": "=26.0.22",
    "cross-env": "=7.0.3",
    "eslint": "=7.22.0",
    "jest": "=26.6.0",
    "nodemon": "^2.0.7",
    "supertest": "=6.1.3"
  }
}
