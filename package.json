{
  "name": "build-week-scaffolding-node",
  "version": "1.0.0",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "migrateh": "heroku run knex migrate:latest -a newtestiss",
    "rollbackh": "heroku run knex migrate:rollback -a newtestiss",
    "databaseh": "heroku pg:psql -a newtestiss",
    "seedh": "heroku run knex seed:run -a newtestiss",
    "test": "cross-env NODE_ENV=testing jest --watchAll --verbose --runInBand",
    "heroku-postbuild": "cd client && npm i && npm run build",
    "deploy": "git push heroku main"
  },
  "engines": {
    "node": "14.16.0",
    "npm": "6.14.11"
  },
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "connect-session-knex": "^2.1.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "helmet": "^4.4.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.3",
    "knex-cleaner": "^1.3.1",
    "pg": "=8.5.1",
    "sqlite3": "^5.0.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/jest": "=26.0.22",
    "cross-env": "=7.0.3",
    "eslint": "=7.22.0",
    "jest": "=26.6.0",
    "nodemon": "^2.0.7",
    "supertest": "=6.1.3"
  }
}
