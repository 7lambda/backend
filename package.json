{
  "name": "build-week-scaffolding-node",
  "version": "1.0.0",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "migrateh": "heroku run knex migrate:latest -a bwfrontend",
    "rollbackh": "heroku run knex migrate:rollback -a bwfrontend",
    "databaseh": "heroku pg:psql -a bwfrontend",
    "seedh": "heroku run knex seed:run -a bwfrontend",
    "test": "cross-env NODE_ENV=testing jest --watchAll --verbose --runInBand",
    "heroku-postbuild": "cd client && npm i && npm run build",
    "deploy": "git push heroku main"
  },
  "engines": {
    "node": "14.16.0",
    "npm": "6.14.11"
  },
  "license": "ISC",
  "dependencies": {
    "cors": "=2.8.5",
    "dotenv": "=8.2.0",
    "express": "=4.17.1",
    "helmet": "=4.4.1",
    "knex": "=0.95.3",
    "knex-cleaner": "=1.3.1",
    "pg": "=8.5.1"
  },
  "devDependencies": {
    "@types/jest": "=26.0.22",
    "cross-env": "=7.0.3",
    "eslint": "=7.22.0",
    "jest": "=26.6.0",
    "nodemon": "=2.0.7",
    "supertest": "=6.1.3"
  }
}
